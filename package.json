{
  "name": "lpff-gui",
  "version": "1.0.0",
  "description": "lpff is a chess gui.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build",
    "package-linux": "electron-builder build --linux appimage",
    "package-win": "electron-builder build --win --x64 --ia32",
    "package-mac": "electron-builder build --mac --x64"
  },
  "keywords": [],
  "author": "andy",
  "license": "MIT",
  "build": {
    "productName": "lpff",
    "appId": "xyz.dumpyy.lpff",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "appimage"

    }
  },
  "dependencies": {
    "@chrisoakman/chessboardjs": "^1.0.0",
    "chess.js": "^1.0.0-beta.3"
  },
  "devDependencies": {
    "electron": "^23.0.0",
    "electron-builder": "^23.6.0"
  }
}
